<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pro.dao.ShowhallMapper">
   <select id="selectShowhalls" resultType="showhall">
      select * from showhall
   </select>
   <select id="selectShowhall" parameterType="int" resultType="showhall">
      select * from showhall where showid=#{showid}
   </select>


</mapper>