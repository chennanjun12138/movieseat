<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pro.dao.ShowmovieMapper">
   <select id="selectShowmovies"  parameterType="int"   resultType="showmovie">
      select * from showmovie where showid=#{showid}
   </select>
   <select id="findShowmovies"   resultType="showmovie">
      select * from showmovie where movieid=#{movieid}
   </select>
</mapper>